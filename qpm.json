{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "AnyText",
    "id": "anytext",
    "version": "1.0.1",
    "url": null,
    "additionalData": {
      "overrideSoName": "libanytext.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "a2l": [
        "pwsh ./scripts/addr2line.ps1 libanytext.so $0"
      ],
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?"
      ],
      "logcat": [
        "pwsh ./scripts/start-logging.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ]
    },
    "ndk": "^27.3.13750724",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "./AnyText.qmod"
  },
  "dependencies": [
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "paper",
      "versionRange": "^3.0.0",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3701.0.0",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.9",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.43",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.10",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^1.4.2",
      "additionalData": {}
    }
  ]
}
